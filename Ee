<script>
function clsCheck(){
	var url = '';
	
	var num = Math.floor(Math.random()*3);
	
	if(num == 0){
		url = 'https://cls6.edunet.net';
	}else if(num == 1){
		url = 'https://cls7.edunet.net';
	}else{
		url = 'https://cls12.edunet.net';
	}
	
	location.href=url+'/cyber/cm/mcom/pmco000z00.do';
}

</script>
